{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "englishNameTranslation": "The Morning Star",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 5932,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 591,
      "translation": "Andolsun o göğe ve Târık'a,"
    },
    {
      "number": 5933,
      "text": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 591,
      "translation": "Târık nedir, bildin mi?"
    },
    {
      "number": 5934,
      "text": "ٱلنَّجْمُ ٱلثَّاقِبُ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 591,
      "translation": "O, karanlığı delen yıldızdır."
    },
    {
      "number": 5935,
      "text": "إِن كُلُّ نَفْسٍۢ لَّمَّا عَلَيْهَا حَافِظٌۭ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 591,
      "translation": "Hiçbir nefis yoktur ki başında bir denetleyici bulunmasın."
    },
    {
      "number": 5936,
      "text": "فَلْيَنظُرِ ٱلْإِنسَٰنُ مِمَّ خُلِقَ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 591,
      "translation": "Onun için insan neden yaratıldığına bir baksın."
    },
    {
      "number": 5937,
      "text": "خُلِقَ مِن مَّآءٍۢ دَافِقٍۢ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 591,
      "translation": "Atılan bir sudan yaratıldı."
    },
    {
      "number": 5938,
      "text": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 591,
      "translation": "O su, erkeğin sulbü ile kadının göğüs kemikleri arasından çıkar."
    },
    {
      "number": 5939,
      "text": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌۭ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 591,
      "translation": "Elbette Allah'ın onu döndürmeye gücü yeter."
    },
    {
      "number": 5940,
      "text": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 591,
      "translation": "O gün bütün sırlar yoklanıp, meydana çıkarılır."
    },
    {
      "number": 5941,
      "text": "فَمَا لَهُۥ مِن قُوَّةٍۢ وَلَا نَاصِرٍۢ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 591,
      "translation": "İnsanın o gün ne bir gücü vardır, ne de bir yardımcısı."
    },
    {
      "number": 5942,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 591,
      "translation": "Andolsun o dönüşlü göğe,"
    },
    {
      "number": 5943,
      "text": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 591,
      "translation": "O yarılıp çatlayan yere,"
    },
    {
      "number": 5944,
      "text": "إِنَّهُۥ لَقَوْلٌۭ فَصْلٌۭ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 591,
      "translation": "Kuşkusuz Kur'ân, ayırıcı bir sözdür."
    },
    {
      "number": 5945,
      "text": "وَمَا هُوَ بِٱلْهَزْلِ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 591,
      "translation": "O asla bir şaka değildir."
    },
    {
      "number": 5946,
      "text": "إِنَّهُمْ يَكِيدُونَ كَيْدًۭا",
      "numberInSurah": 15,
      "juz": 30,
      "page": 591,
      "translation": "Haberin olsun ki, kâfirler hep hile kuruyorlar."
    },
    {
      "number": 5947,
      "text": "وَأَكِيدُ كَيْدًۭا",
      "numberInSurah": 16,
      "juz": 30,
      "page": 591,
      "translation": "Ben de hilelerine karşılık veririm."
    },
    {
      "number": 5948,
      "text": "فَمَهِّلِ ٱلْكَٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا",
      "numberInSurah": 17,
      "juz": 30,
      "page": 591,
      "translation": "Onun için sen kâfirlere mühlet ver, onlara az bir zaman tanı."
    }
  ]
}