{
  "number": 105,
  "name": "سُورَةُ الفِيلِ",
  "englishName": "Al-Fil",
  "englishNameTranslation": "The Elephant",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 6189,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَٰبِ ٱلْفِيلِ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 601,
      "translation": "Görmedin mi Rabb'in fil sahiplerine ne yaptı?"
    },
    {
      "number": 6190,
      "text": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِى تَضْلِيلٍۢ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 601,
      "translation": "Onların tuzaklarını boşa çıkarmadı mı?"
    },
    {
      "number": 6191,
      "text": "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 601,
      "translation": "Üzerlerine sürü sürü kuşlar gönderdi."
    },
    {
      "number": 6192,
      "text": "تَرْمِيهِم بِحِجَارَةٍۢ مِّن سِجِّيلٍۢ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 601,
      "translation": "Onlara çamurdan sertleşmiş taşlar atıyorlardı."
    },
    {
      "number": 6193,
      "text": "فَجَعَلَهُمْ كَعَصْفٍۢ مَّأْكُولٍۭ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 601,
      "translation": "Ve onları, yenilmiş ekin yaprağı gibi yaptı."
    }
  ]
