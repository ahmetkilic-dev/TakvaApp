  "ayahs": [
    {
      "number": 6217,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ تَبَّتْ يَدَآ أَبِى لَهَبٍۢ وَتَبَّ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 603,
      "translation": "Ebu Leheb'in elleri kurusun (yok olsun o), zaten yok oldu ya."
    },
    {
      "number": 6218,
      "text": "مَآ أَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 603,
      "translation": "Ne malı ne de kazandığı onu kurtaramadı."
    },
    {
      "number": 6219,
      "text": "سَيَصْلَىٰ نَارًۭا ذَاتَ لَهَبٍۢ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 603,
      "translation": "(O), alevli bir ateşe girecektir."
    },
    {
      "number": 6220,
      "text": "وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 603,
      "translation": "Karısı da odun hamalı olarak (onunla beraber girecektir)."
    },
    {
      "number": 6221,
      "text": "فِى جِيدِهَا حَبْلٌۭ مِّن مَّسَدٍۭ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 603,
      "translation": "Boynunda da hurma lifinden bir ip olacaktır."
    }
  ]
