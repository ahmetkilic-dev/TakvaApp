{
  "number": 113,
  "name": "سُورَةُ الفَلَقِ",
  "englishName": "Al-Falaq",
  "englishNameTranslation": "The Dawn",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 6226,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ ٱلْفَلَقِ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 604,
      "translation": "De ki: \"Ben, ağaran sabahın Rabbine sığınırım,"
    },
    {
      "number": 6227,
      "text": "مِن شَرِّ مَا خَلَقَ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 604,
      "translation": "Yarattığı şeylerin şerrinden,"
    },
    {
      "number": 6228,
      "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 604,
      "translation": "Karanlığı çöktüğü zaman gecenin şerrinden,"
    },
    {
      "number": 6229,
      "text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِى ٱلْعُقَدِ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 604,
      "translation": "Ve düğümlere üfleyen büyücülerin şerrinden,"
    },
    {
      "number": 6230,
      "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 604,
      "translation": "Ve hased ettiği zaman hasetçinin şerrinden."
    }
  ]
}