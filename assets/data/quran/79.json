{
  "number": 79,
  "name": "سُورَةُ النَّازِعَاتِ",
  "englishName": "An-Naazi'aat",
  "englishNameTranslation": "Those who drag forth",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 5713,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلنَّٰزِعَٰتِ غَرْقًۭا",
      "numberInSurah": 1,
      "juz": 30,
      "page": 583,
      "translation": "Andolsun şiddetle çekip çıkaranlara,"
    },
    {
      "number": 5714,
      "text": "وَٱلنَّٰشِطَٰتِ نَشْطًۭا",
      "numberInSurah": 2,
      "juz": 30,
      "page": 583,
      "translation": "Usulcacık çekenlere,"
    },
    {
      "number": 5715,
      "text": "وَٱلسَّٰبِحَٰتِ سَبْحًۭا",
      "numberInSurah": 3,
      "juz": 30,
      "page": 583,
      "translation": "Yüzüp yüzüp gidenlere,"
    },
    {
      "number": 5716,
      "text": "فَٱلسَّٰبِقَٰتِ سَبْقًۭا",
      "numberInSurah": 4,
      "juz": 30,
      "page": 583,
      "translation": "Yarışıp geçenlere,"
    },
    {
      "number": 5717,
      "text": "فَٱلْمُدَبِّرَٰتِ أَمْرًۭا",
      "numberInSurah": 5,
      "juz": 30,
      "page": 583,
      "translation": "Derken bir iş çevirenlere kasem olsun (ki kıyamet var)."
    },
    {
      "number": 5718,
      "text": "يَوْمَ تَرْجُفُ ٱلرَّاجِفَةُ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 583,
      "translation": "O gün deprem sarsar,"
    },
    {
      "number": 5719,
      "text": "تَتْبَعُهَا ٱلرَّادِفَةُ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 583,
      "translation": "Onu ikinci bir sarsıntı izler."
    },
    {
      "number": 5720,
      "text": "قُلُوبٌۭ يَوْمَئِذٍۢ وَاجِفَةٌ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 583,
      "translation": "Yürekler vardır, o gün kaygıdan hoplar."
    },
    {
      "number": 5721,
      "text": "أَبْصَٰرُهَا خَٰشِعَةٌۭ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 583,
      "translation": "Gözler kalkmaz saygıdan."
    },
    {
      "number": 5722,
      "text": "يَقُولُونَ أَءِنَّا لَمَرْدُودُونَ فِى ٱلْحَافِرَةِ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 583,
      "translation": "Diyorlar ki: \"Biz tekrar eski halimize mi döndürülecekmişiz?"
    },
    {
      "number": 5723,
      "text": "أَءِذَا كُنَّا عِظَٰمًۭا نَّخِرَةًۭ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 583,
      "translation": "\"Biz, çürümüş kemikler olduktan sonra ha?\""
    },
    {
      "number": 5724,
      "text": "قَالُوا۟ تِلْكَ إِذًۭا كَرَّةٌ خَاسِرَةٌۭ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 583,
      "translation": "\"Öyleyse bu çok zararlı bir dönüştür.\" dediler."
    },
    {
      "number": 5725,
      "text": "فَإِنَّمَا هِىَ زَجْرَةٌۭ وَٰحِدَةٌۭ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 583,
      "translation": "Fakat o bir tek haykırıştır."
    },
    {
      "number": 5726,
      "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 583,
      "translation": "Bir de bakarsın hepsi meydandadır."
    },
    {
      "number": 5727,
      "text": "هَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
      "numberInSurah": 15,
      "juz": 30,
      "page": 583,
      "translation": "Musa'nın haberi sana geldi mi?"
    },
    {
      "number": 5728,
      "text": "إِذْ نَادَىٰهُ رَبُّهُۥ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى",
      "numberInSurah": 16,
      "juz": 30,
      "page": 584,
      "translation": "Hani Rabbi ona kutsal vaadi Tuva'da seslenmişti:"
    },
    {
      "number": 5729,
      "text": "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ",
      "numberInSurah": 17,
      "juz": 30,
      "page": 584,
      "translation": "\"Haydi, demişti, git Firavun'a, çünkü o çok azdı.\""
    },
    {
      "number": 5730,
      "text": "فَقُلْ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
      "numberInSurah": 18,
      "juz": 30,
      "page": 584,
      "translation": "De ki: İster misin arınasın?"
    },
    {
      "number": 5731,
      "text": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
      "numberInSurah": 19,
      "juz": 30,
      "page": 584,
      "translation": "Seni Rabbinin yoluna ileteyim de ondan korkasın."
    },
    {
      "number": 5732,
      "text": "فَأَرَىٰهُ ٱلْءَايَةَ ٱلْكُبْرَىٰ",
      "numberInSurah": 20,
      "juz": 30,
      "page": 584,
      "translation": "Musa Firavun'a o büyük mucizeyi gösterdi."
    },
    {
      "number": 5733,
      "text": "فَكَذَّبَ وَعَصَىٰ",
      "numberInSurah": 21,
      "juz": 30,
      "page": 584,
      "translation": "Fakat Firavun yalanladı, karşı geldi."
    },
    {
      "number": 5734,
      "text": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
      "numberInSurah": 22,
      "juz": 30,
      "page": 584,
      "translation": "Sonra koşarak dönüp gitti."
    },
    {
      "number": 5735,
      "text": "فَحَشَرَ فَنَادَىٰ",
      "numberInSurah": 23,
      "juz": 30,
      "page": 584,
      "translation": "Derken adamlarını topladı da bağırdı:"
    },
    {
      "number": 5736,
      "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلْأَعْلَىٰ",
      "numberInSurah": 24,
      "juz": 30,
      "page": 584,
      "translation": "\"Ben sizin en yüce Rabbinizim\" dedi."
    },
    {
      "number": 5737,
      "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلْءَاخِرَةِ وَٱلْأُولَىٰٓ",
      "numberInSurah": 25,
      "juz": 30,
      "page": 584,
      "translation": "Allah da onu tuttu, dünya ve ahiret azabıyla yakalayıverdi."
    },
    {
      "number": 5738,
      "text": "إِنَّ فِى ذَٰلِكَ لَعِبْرَةًۭ لِّمَن يَخْشَىٰٓ",
      "numberInSurah": 26,
      "juz": 30,
      "page": 584,
      "translation": "Kuşkusuz bunda, saygı duyacaklar için bir ibret vardır."
    },
    {
      "number": 5739,
      "text": "ءَأَنتُمْ أَشَدُّ خَلْقًا أَمِ ٱلسَّمَآءُ ۚ بَنَىٰهَا",
      "numberInSurah": 27,
      "juz": 30,
      "page": 584,
      "translation": "Yaratılışça siz mi daha çetinsiniz, yoksa gök mü? Onu Allah bina etti."
    },
    {
      "number": 5740,
      "text": "رَفَعَ سَمْكَهَا فَسَوَّىٰهَا",
      "numberInSurah": 28,
      "juz": 30,
      "page": 584,
      "translation": "Tavanını yükseltti, onu bir düzene koydu."
    },
    {
      "number": 5741,
      "text": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَىٰهَا",
      "numberInSurah": 29,
      "juz": 30,
      "page": 584,
      "translation": "Gecesini kararttı, kuşluğunu çıkardı."
    },
    {
      "number": 5742,
      "text": "وَٱلْأَرْضَ بَعْدَ ذَٰلِكَ دَحَىٰهَآ",
      "numberInSurah": 30,
      "juz": 30,
      "page": 584,
      "translation": "Bundan sonra da yeryüzünü döşedi."
    },
    {
      "number": 5743,
      "text": "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَىٰهَا",
      "numberInSurah": 31,
      "juz": 30,
      "page": 584,
      "translation": "Ondan suyunu ve otlağını çıkardı."
    },
    {
      "number": 5744,
      "text": "وَٱلْجِبَالَ أَرْسَىٰهَا",
      "numberInSurah": 32,
      "juz": 30,
      "page": 584,
      "translation": "Dağlarını oturttu."
    },
    {
      "number": 5745,
      "text": "مَتَٰعًۭا لَّكُمْ وَلِأَنْعَٰمِكُمْ",
      "numberInSurah": 33,
      "juz": 30,
      "page": 584,
      "translation": "Sizin ve hayvanlarınızın geçimi için."
    },
    {
      "number": 5746,
      "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلْكُبْرَىٰ",
      "numberInSurah": 34,
      "juz": 30,
      "page": 584,
      "translation": "Fakat o her şeyi bastıran büyük felaket geldiği vakit,"
    },
    {
      "number": 5747,
      "text": "يَوْمَ يَتَذَكَّرُ ٱلْإِنسَٰنُ مَا سَعَىٰ",
      "numberInSurah": 35,
      "juz": 30,
      "page": 584,
      "translation": "O, insanın neyin peşinde koştuğunu anladığı gün,"
    },
    {
      "number": 5748,
      "text": "وَبُرِّزَتِ ٱلْجَحِيمُ لِمَن يَرَىٰ",
      "numberInSurah": 36,
      "juz": 30,
      "page": 584,
      "translation": "Gören kimseler için cehennem hortlatıldığı vakit,"
    },
    {
      "number": 5749,
      "text": "فَأَمَّا مَن طَغَىٰ",
      "numberInSurah": 37,
      "juz": 30,
      "page": 584,
      "translation": "Artık her kim azgınlık etmiş,"
    },
    {
      "number": 5750,
      "text": "وَءَاثَرَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
      "numberInSurah": 38,
      "juz": 30,
      "page": 584,
      "translation": "Ve dünya hayatını tercih etmişse,"
    },
    {
      "number": 5751,
      "text": "فَإِنَّ ٱلْجَحِيمَ هِىَ ٱلْمَأْوَىٰ",
      "numberInSurah": 39,
      "juz": 30,
      "page": 584,
      "translation": "Kuşkusuz onun varacağı yer cehennemdir."
    },
    {
      "number": 5752,
      "text": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفْسَ عَنِ ٱلْهَوَىٰ",
      "numberInSurah": 40,
      "juz": 30,
      "page": 584,
      "translation": "Kim de Rabbinin divanında durmaktan korkmuş, nefsini boş heveslerden menetmiş ise,"
    },
    {
      "number": 5753,
      "text": "فَإِنَّ ٱلْجَنَّةَ هِىَ ٱلْمَأْوَىٰ",
      "numberInSurah": 41,
      "juz": 30,
      "page": 584,
      "translation": "Kuşkusuz onun varacağı yer cennettir."
    },
    {
      "number": 5754,
      "text": "يَسْـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرْسَىٰهَا",
      "numberInSurah": 42,
      "juz": 30,
      "page": 584,
      "translation": "Sana o kıyameti soruyorlar, ne zaman kopacak diye."
    },
    {
      "number": 5755,
      "text": "فِيمَ أَنتَ مِن ذِكْرَىٰهَآ",
      "numberInSurah": 43,
      "juz": 30,
      "page": 584,
      "translation": "Sen nerde, onu anlatmak nerde?!"
    },
    {
      "number": 5756,
      "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
      "numberInSurah": 44,
      "juz": 30,
      "page": 584,
      "translation": "Onun son ilmi Rabbine aittir."
    },
    {
      "number": 5757,
      "text": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخْشَىٰهَا",
      "numberInSurah": 45,
      "juz": 30,
      "page": 584,
      "translation": "Sen ancak ondan korkacak olanları uyarıcısın."
    },
    {
      "number": 5758,
      "text": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوٓا۟ إِلَّا عَشِيَّةً أَوْ ضُحَىٰهَا",
      "numberInSurah": 46,
      "juz": 30,
      "page": 584,
      "translation": "Onlar o kıyameti görecekleri gün sanki dünyada bir akşam veya kuşluğundan başka durmamışa dönecekler."
    }
  ]
}