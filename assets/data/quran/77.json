{
  "number": 77,
  "name": "سُورَةُ المُرۡسَلَاتِ",
  "englishName": "Al-Mursalaat",
  "englishNameTranslation": "The Emissaries",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 5623,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلْمُرْسَلَٰتِ عُرْفًۭا",
      "numberInSurah": 1,
      "juz": 29,
      "page": 580,
      "translation": "Andolsun birbiri ardınca gönderilenlere,"
    },
    {
      "number": 5624,
      "text": "فَٱلْعَٰصِفَٰتِ عَصْفًۭا",
      "numberInSurah": 2,
      "juz": 29,
      "page": 580,
      "translation": "Büküp devirenlere,"
    },
    {
      "number": 5625,
      "text": "وَٱلنَّٰشِرَٰتِ نَشْرًۭا",
      "numberInSurah": 3,
      "juz": 29,
      "page": 580,
      "translation": "Yaydıkça yayanlara,"
    },
    {
      "number": 5626,
      "text": "فَٱلْفَٰرِقَٰتِ فَرْقًۭا",
      "numberInSurah": 4,
      "juz": 29,
      "page": 580,
      "translation": "Seçip ayıranlara,"
    },
    {
      "number": 5627,
      "text": "فَٱلْمُلْقِيَٰتِ ذِكْرًا",
      "numberInSurah": 5,
      "juz": 29,
      "page": 580,
      "translation": "Bir öğüt bırakanlara,"
    },
    {
      "number": 5628,
      "text": "عُذْرًا أَوْ نُذْرًا",
      "numberInSurah": 6,
      "juz": 29,
      "page": 580,
      "translation": "Gerek özür için olsun, gerek uyarı için,"
    },
    {
      "number": 5629,
      "text": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌۭ",
      "numberInSurah": 7,
      "juz": 29,
      "page": 580,
      "translation": "Herhalde size vaad olunan kesinlikle olacaktır."
    },
    {
      "number": 5630,
      "text": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ",
      "numberInSurah": 8,
      "juz": 29,
      "page": 580,
      "translation": "Hani o yıldızlar silindiği zaman,"
    },
    {
      "number": 5631,
      "text": "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ",
      "numberInSurah": 9,
      "juz": 29,
      "page": 580,
      "translation": "Gök yarıldığı zaman,"
    },
    {
      "number": 5632,
      "text": "وَإِذَا ٱلْجِبَالُ نُسِفَتْ",
      "numberInSurah": 10,
      "juz": 29,
      "page": 580,
      "translation": "Dağlar savrulduğu zaman,"
    },
    {
      "number": 5633,
      "text": "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ",
      "numberInSurah": 11,
      "juz": 29,
      "page": 580,
      "translation": "Elçiler, tayin edilen vakitlerine erdirildikleri zaman,"
    },
    {
      "number": 5634,
      "text": "لِأَىِّ يَوْمٍ أُجِّلَتْ",
      "numberInSurah": 12,
      "juz": 29,
      "page": 580,
      "translation": "Bunlar hangi güne ertelendiler?"
    },
    {
      "number": 5635,
      "text": "لِيَوْمِ ٱلْفَصْلِ",
      "numberInSurah": 13,
      "juz": 29,
      "page": 580,
      "translation": "Hüküm gününe.."
    },
    {
      "number": 5636,
      "text": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ",
      "numberInSurah": 14,
      "juz": 29,
      "page": 580,
      "translation": "Bildin mi, nedir o hüküm günü?"
    },
    {
      "number": 5637,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 15,
      "juz": 29,
      "page": 580,
      "translation": "O gün yalanlayanların vay haline!"
    },
    {
      "number": 5638,
      "text": "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ",
      "numberInSurah": 16,
      "juz": 29,
      "page": 580,
      "translation": "Biz, öncekileri helak etmedik mi?"
    },
    {
      "number": 5639,
      "text": "ثُمَّ نُتْبِعُهُمُ ٱلْءَاخِرِينَ",
      "numberInSurah": 17,
      "juz": 29,
      "page": 580,
      "translation": "Sonra geridekileri de onlara katarız."
    },
    {
      "number": 5640,
      "text": "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
      "numberInSurah": 18,
      "juz": 29,
      "page": 580,
      "translation": "Biz suçlulara böyle yaparız."
    },
    {
      "number": 5641,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 19,
      "juz": 29,
      "page": 580,
      "translation": "O gün yalanlayanların vah haline!"
    },
    {
      "number": 5642,
      "text": "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍۢ مَّهِينٍۢ",
      "numberInSurah": 20,
      "juz": 29,
      "page": 581,
      "translation": "Biz sizi âdi bir sudan yaratmadık mı?"
    },
    {
      "number": 5643,
      "text": "فَجَعَلْنَٰهُ فِى قَرَارٍۢ مَّكِينٍ",
      "numberInSurah": 21,
      "juz": 29,
      "page": 581,
      "translation": "Onu sağlam bir yerde oturttuk."
    },
    {
      "number": 5644,
      "text": "إِلَىٰ قَدَرٍۢ مَّعْلُومٍۢ",
      "numberInSurah": 22,
      "juz": 29,
      "page": 581,
      "translation": "Belli bir süreye kadar."
    },
    {
      "number": 5645,
      "text": "فَقَدَرْنَا فَنِعْمَ ٱلْقَٰدِرُونَ",
      "numberInSurah": 23,
      "juz": 29,
      "page": 581,
      "translation": "Demek ki biçimlendirmişiz. Ne güzel biçimlendireniz biz."
    },
    {
      "number": 5646,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 24,
      "juz": 29,
      "page": 581,
      "translation": "O gün yalanlayanların vay haline!"
    },
    {
      "number": 5647,
      "text": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا",
      "numberInSurah": 25,
      "juz": 29,
      "page": 581,
      "translation": "Yeryüzünü bir tokat (toplanma yeri) yapmadık mı?"
    },
    {
      "number": 5648,
      "text": "أَحْيَآءًۭ وَأَمْوَٰتًۭا",
      "numberInSurah": 26,
      "juz": 29,
      "page": 581,
      "translation": "Gerek diriler, gerekse ölüler için."
    },
    {
      "number": 5649,
      "text": "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَٰمِخَٰتٍۢ وَأَسْقَيْنَٰكُم مَّآءًۭ فُرَاتًۭا",
      "numberInSurah": 27,
      "juz": 29,
      "page": 581,
      "translation": "Orada yüksek yüksek dağlar oturtup da size bir tatlı su sunmadık mı?"
    },
    {
      "number": 5650,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 28,
      "juz": 29,
      "page": 581,
      "translation": "O gün yalanlayanların vay haline!"
    },
    {
      "number": 5651,
      "text": "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
      "numberInSurah": 29,
      "juz": 29,
      "page": 581,
      "translation": "(Kıyameti yalanlayanlara şöyle denir): \"Haydin gidin o yalanladığınız şeye doğru.\""
    },
    {
      "number": 5652,
      "text": "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّۢ ذِى ثَلَٰثِ شُعَبٍۢ",
      "numberInSurah": 30,
      "juz": 29,
      "page": 581,
      "translation": "\"Haydi gidin o üç çatallı gölgeye (cehenneme).\""
    },
    {
      "number": 5653,
      "text": "لَّا ظَلِيلٍۢ وَلَا يُغْنِى مِنَ ٱللَّهَبِ",
      "numberInSurah": 31,
      "juz": 29,
      "page": 581,
      "translation": "O, ne gölgelendirir, ne alevden korur."
    },
    {
      "number": 5654,
      "text": "إِنَّهَا تَرْمِى بِشَرَرٍۢ كَٱلْقَصْرِ",
      "numberInSurah": 32,
      "juz": 29,
      "page": 581,
      "translation": "O, saray gibi kıvılcımlar atar."
    },
    {
      "number": 5655,
      "text": "كَأَنَّهُۥ جِمَٰلَتٌۭ صُفْرٌۭ",
      "numberInSurah": 33,
      "juz": 29,
      "page": 581,
      "translation": "Sanki o kıvılcımlar, sarı sarı (erkek deve sürüleridir)."
    },
    {
      "number": 5656,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 34,
      "juz": 29,
      "page": 581,
      "translation": "O gün yalanlayanların vay haline!"
    },
    {
      "number": 5657,
      "text": "هَٰذَا يَوْمُ لَا يَنطِقُونَ",
      "numberInSurah": 35,
      "juz": 29,
      "page": 581,
      "translation": "Bugün, konuşamıyacakları gündür."
    },
    {
      "number": 5658,
      "text": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
      "numberInSurah": 36,
      "juz": 29,
      "page": 581,
      "translation": "Kendilerine izin de verilmez ki, özür beyan etsinler."
    },
    {
      "number": 5659,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 37,
      "juz": 29,
      "page": 581,
      "translation": "O gün yalanlayanların vay haline!"
    },
    {
      "number": 5660,
      "text": "هَٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَٰكُمْ وَٱلْأَوَّلِينَ",
      "numberInSurah": 38,
      "juz": 29,
      "page": 581,
      "translation": "Bu, işte o hüküm günüdür. Sizi ve öncekileri bir araya topladık."
    },
    {
      "number": 5661,
      "text": "فَإِن كَانَ لَكُمْ كَيْدٌۭ فَكِيدُونِ",
      "numberInSurah": 39,
      "juz": 29,
      "page": 581,
      "translation": "Bir hileniz varsa beni atlatın."
    },
    {
      "number": 5662,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 40,
      "juz": 29,
      "page": 581,
      "translation": "O gün yalanlayanların vay haline!"
    },
    {
      "number": 5663,
      "text": "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَٰلٍۢ وَعُيُونٍۢ",
      "numberInSurah": 41,
      "juz": 29,
      "page": 581,
      "translation": "Kuşkusuz takva sahipleri gölgeler altında ve pınar başlarındadır."
    },
    {
      "number": 5664,
      "text": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ",
      "numberInSurah": 42,
      "juz": 29,
      "page": 581,
      "translation": "Canlarının çektiğinden türlü meyveler arasındadırlar."
    },
    {
      "number": 5665,
      "text": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
      "numberInSurah": 43,
      "juz": 29,
      "page": 581,
      "translation": "(Onlara): \"Yaptıklarınıza karşılık afiyetle yiyin, için\" (denir)."
    },
    {
      "number": 5666,
      "text": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
      "numberInSurah": 44,
      "juz": 29,
      "page": 581,
      "translation": "İşte biz güzel amel işleyenleri böyle mükafatlandırırız."
    },
    {
      "number": 5667,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 45,
      "juz": 29,
      "page": 581,
      "translation": "O gün yalanlayanların vay haline!"
    },
    {
      "number": 5668,
      "text": "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ",
      "numberInSurah": 46,
      "juz": 29,
      "page": 581,
      "translation": "Yiyin, zevklenin biraz, çünkü siz suçlularsınız."
    },
    {
      "number": 5669,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 47,
      "juz": 29,
      "page": 581,
      "translation": "O gün yalanlayanların vay haline!"
    },
    {
      "number": 5670,
      "text": "وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ",
      "numberInSurah": 48,
      "juz": 29,
      "page": 581,
      "translation": "Onlara: \"Rüku edin\" denildiği zaman etmezler."
    },
    {
      "number": 5671,
      "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
      "numberInSurah": 49,
      "juz": 29,
      "page": 581,
      "translation": "Vay haline o gün yalanlayanların!"
    },
    {
      "number": 5672,
      "text": "فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ",
      "numberInSurah": 50,
      "juz": 29,
      "page": 581,
      "translation": "Artık bundan (Kur'an'dan) sonra hangi söze inanacaklar?"
    }
  ]
}