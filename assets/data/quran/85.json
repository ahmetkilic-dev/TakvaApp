{
  "number": 85,
  "name": "سُورَةُ البُرُوجِ",
  "englishName": "Al-Burooj",
  "englishNameTranslation": "The Constellations",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 5910,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 590,
      "translation": "Burçlar sahibi gökyüzüne,"
    },
    {
      "number": 5911,
      "text": "وَٱلْيَوْمِ ٱلْمَوْعُودِ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 590,
      "translation": "Vaad olunan o güne,"
    },
    {
      "number": 5912,
      "text": "وَشَاهِدٍۢ وَمَشْهُودٍۢ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 590,
      "translation": "Şahitlik edene ve edilene andolsun ki,"
    },
    {
      "number": 5913,
      "text": "قُتِلَ أَصْحَٰبُ ٱلْأُخْدُودِ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 590,
      "translation": "Kahroldu o hendeğin sahipleri,"
    },
    {
      "number": 5914,
      "text": "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 590,
      "translation": "O çıralı ateşin,"
    },
    {
      "number": 5915,
      "text": "إِذْ هُمْ عَلَيْهَا قُعُودٌۭ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 590,
      "translation": "Hani o ateşin başına oturmuşlar,"
    },
    {
      "number": 5916,
      "text": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌۭ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 590,
      "translation": "Müminlere yaptıklarını seyrediyorlardı."
    },
    {
      "number": 5917,
      "text": "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 590,
      "translation": "Müminlere kızmalarının sebebi de, onların yalnız çok güçlü ve övgüye lâyık olan Allah'a iman etmeleri idi."
    },
    {
      "number": 5918,
      "text": "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍۢ شَهِيدٌ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 590,
      "translation": "O Allah ki, göklerin ve yerin hükümranlığı O'nundur ve Allah her şeye şahittir."
    },
    {
      "number": 5919,
      "text": "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 590,
      "translation": "İnanan erkek ve kadınlara işkence yapıp sonra da tevbe etmeyenlere cehennem azabı ve yangın azabı vardır."
    },
    {
      "number": 5920,
      "text": "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ لَهُمْ جَنَّٰتٌۭ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 590,
      "translation": "İnanan ve iyi amel yapanlar için de altından ırmaklar akan cennetler vardır. İşte büyük kurtuluş odur."
    },
    {
      "number": 5921,
      "text": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 590,
      "translation": "Kuşkusuz Rabbinin yakalaması serttir."
    },
    {
      "number": 5922,
      "text": "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 590,
      "translation": "Yoktan o yaratır ve tekrar o diriltir."
    },
    {
      "number": 5923,
      "text": "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 590,
      "translation": "Bununla beraber çok bağışlayandır, çok sevendir."
    },
    {
      "number": 5924,
      "text": "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ",
      "numberInSurah": 15,
      "juz": 30,
      "page": 590,
      "translation": "Arş'ın sahibidir, yücedir."
    },
    {
      "number": 5925,
      "text": "فَعَّالٌۭ لِّمَا يُرِيدُ",
      "numberInSurah": 16,
      "juz": 30,
      "page": 590,
      "translation": "Dilediğini yapandır."
    },
    {
      "number": 5926,
      "text": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ",
      "numberInSurah": 17,
      "juz": 30,
      "page": 590,
      "translation": "O orduların kıssası sana geldi mi?"
    },
    {
      "number": 5927,
      "text": "فِرْعَوْنَ وَثَمُودَ",
      "numberInSurah": 18,
      "juz": 30,
      "page": 590,
      "translation": "Yani Firavun ve Semud'un?"
    },
    {
      "number": 5928,
      "text": "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى تَكْذِيبٍۢ",
      "numberInSurah": 19,
      "juz": 30,
      "page": 590,
      "translation": "Fakat o inkarcılar hâlâ bir yalanlama içinde."
    },
    {
      "number": 5929,
      "text": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطٌۢ",
      "numberInSurah": 20,
      "juz": 30,
      "page": 590,
      "translation": "Oysa Allah onları arkalarından kuşatmıştır."
    },
    {
      "number": 5930,
      "text": "بَلْ هُوَ قُرْءَانٌۭ مَّجِيدٌۭ",
      "numberInSurah": 21,
      "juz": 30,
      "page": 590,
      "translation": "Hayır o şerefli bir Kur'ân'dır."
    },
    {
      "number": 5931,
      "text": "فِى لَوْحٍۢ مَّحْفُوظٍۭ",
      "numberInSurah": 22,
      "juz": 30,
      "page": 590,
      "translation": "Levhi Mahfuz'dadır."
    }
  ]
}