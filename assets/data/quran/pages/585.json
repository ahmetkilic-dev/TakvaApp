[
  {
    "id": 5759,
    "arabic": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ عَبَسَ وَتَوَلَّىٰٓ",
    "turkish": "(Peygamber) Yüzünü ekşitti ve döndü.",
    "verseNumber": 1,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5760,
    "arabic": "أَن جَآءَهُ ٱلْأَعْمَىٰ",
    "turkish": "Kendisine âmâ geldi, diye.",
    "verseNumber": 2,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5761,
    "arabic": "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
    "turkish": "Ne bilirsin, belki o temizlenecek?",
    "verseNumber": 3,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5762,
    "arabic": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ",
    "turkish": "Veya öğüt belleyecek de öğüt ona fayda verecek.",
    "verseNumber": 4,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5763,
    "arabic": "أَمَّا مَنِ ٱسْتَغْنَىٰ",
    "turkish": "Ama buna ihtiyaç hissetmeyene gelince,",
    "verseNumber": 5,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5764,
    "arabic": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
    "turkish": "Sen ona yöneliyorsun.",
    "verseNumber": 6,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5765,
    "arabic": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
    "turkish": "Onun temizlenmemesinden sana ne?",
    "verseNumber": 7,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5766,
    "arabic": "وَأَمَّا مَن جَآءَكَ يَسْعَىٰ",
    "turkish": "Ama sana can atarak gelen,",
    "verseNumber": 8,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5767,
    "arabic": "وَهُوَ يَخْشَىٰ",
    "turkish": "Allah'tan korkarak gelmişken,",
    "verseNumber": 9,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5768,
    "arabic": "فَأَنتَ عَنْهُ تَلَهَّىٰ",
    "turkish": "Sen onunla ilgilenmiyorsun.",
    "verseNumber": 10,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5769,
    "arabic": "كَلَّآ إِنَّهَا تَذْكِرَةٌۭ",
    "turkish": "Hayır hayır, sakın. Çünkü o Kur'ân bir öğüttür.",
    "verseNumber": 11,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5770,
    "arabic": "فَمَن شَآءَ ذَكَرَهُۥ",
    "turkish": "Artık dileyen onu düşünür.",
    "verseNumber": 12,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5771,
    "arabic": "فِى صُحُفٍۢ مُّكَرَّمَةٍۢ",
    "turkish": "O, değerli sahifelerdedir.",
    "verseNumber": 13,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5772,
    "arabic": "مَّرْفُوعَةٍۢ مُّطَهَّرَةٍۭ",
    "turkish": "Yüksek tutulan tertemiz sahifelerde.",
    "verseNumber": 14,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5773,
    "arabic": "بِأَيْدِى سَفَرَةٍۢ",
    "turkish": "Yazıcıların ellerindedir,",
    "verseNumber": 15,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5774,
    "arabic": "كِرَامٍۭ بَرَرَةٍۢ",
    "turkish": "Değerli, iyi yazıcıların.",
    "verseNumber": 16,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5775,
    "arabic": "قُتِلَ ٱلْإِنسَٰنُ مَآ أَكْفَرَهُۥ",
    "turkish": "O kahrolası insan, ne nankör şey.",
    "verseNumber": 17,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5776,
    "arabic": "مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ",
    "turkish": "O yaratan onu hangi şeyden yarattı?",
    "verseNumber": 18,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5777,
    "arabic": "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
    "turkish": "Bir damla sudan, onu yarattı da biçime koydu.",
    "verseNumber": 19,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5778,
    "arabic": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
    "turkish": "Sonra ona yolunu kolaylaştırdı.",
    "verseNumber": 20,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5779,
    "arabic": "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ",
    "turkish": "Sonra onu öldürdü de kabre koydurdu.",
    "verseNumber": 21,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5780,
    "arabic": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
    "turkish": "Sonra dilediği vakit onu tekrar diriltir.",
    "verseNumber": 22,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5781,
    "arabic": "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ",
    "turkish": "Hayır hayır, doğrusu o, hiç Allah'ın emrini tam yerine getirmedi,",
    "verseNumber": 23,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5782,
    "arabic": "فَلْيَنظُرِ ٱلْإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ",
    "turkish": "Bir de o insan yiyeceğine baksın.",
    "verseNumber": 24,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5783,
    "arabic": "أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّۭا",
    "turkish": "Biz o suyu bol bol döktük.",
    "verseNumber": 25,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5784,
    "arabic": "ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّۭا",
    "turkish": "Sonra toprağı nasıl da yardık.",
    "verseNumber": 26,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5785,
    "arabic": "فَأَنۢبَتْنَا فِيهَا حَبًّۭا",
    "turkish": "Bu suretle orada ekinler bitirdik.",
    "verseNumber": 27,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5786,
    "arabic": "وَعِنَبًۭا وَقَضْبًۭا",
    "turkish": "Üzümler, yoncalar,",
    "verseNumber": 28,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5787,
    "arabic": "وَزَيْتُونًۭا وَنَخْلًۭا",
    "turkish": "Zeytinlikler, hurmalıklar,",
    "verseNumber": 29,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5788,
    "arabic": "وَحَدَآئِقَ غُلْبًۭا",
    "turkish": "İri ve sık ağaçlı bahçeler,",
    "verseNumber": 30,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5789,
    "arabic": "وَفَٰكِهَةًۭ وَأَبًّۭا",
    "turkish": "Meyveler, çayırlar bitirdik.",
    "verseNumber": 31,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5790,
    "arabic": "مَّتَٰعًۭا لَّكُمْ وَلِأَنْعَٰمِكُمْ",
    "turkish": "Siz ve hayvanlarınız faydalansın diye.",
    "verseNumber": 32,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5791,
    "arabic": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
    "turkish": "Kulakları sağır eden o gürültü geldiğinde,",
    "verseNumber": 33,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5792,
    "arabic": "يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ",
    "turkish": "O gün kişi kaçar, kardeşinden...",
    "verseNumber": 34,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5793,
    "arabic": "وَأُمِّهِۦ وَأَبِيهِ",
    "turkish": "Anasından, babasından..",
    "verseNumber": 35,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5794,
    "arabic": "وَصَٰحِبَتِهِۦ وَبَنِيهِ",
    "turkish": "Eşinden ve oğullarından.",
    "verseNumber": 36,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5795,
    "arabic": "لِكُلِّ ٱمْرِئٍۢ مِّنْهُمْ يَوْمَئِذٍۢ شَأْنٌۭ يُغْنِيهِ",
    "turkish": "Onlardan her birinin o gün başından aşan işi vardır.",
    "verseNumber": 37,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5796,
    "arabic": "وُجُوهٌۭ يَوْمَئِذٍۢ مُّسْفِرَةٌۭ",
    "turkish": "Yüzler var ki, o gün parıl parıl,",
    "verseNumber": 38,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5797,
    "arabic": "ضَاحِكَةٌۭ مُّسْتَبْشِرَةٌۭ",
    "turkish": "Güler, sevinir.",
    "verseNumber": 39,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5798,
    "arabic": "وَوُجُوهٌۭ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌۭ",
    "turkish": "Yüzler de var ki, o gün tozlanmış,",
    "verseNumber": 40,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5799,
    "arabic": "تَرْهَقُهَا قَتَرَةٌ",
    "turkish": "Onları karanlık bürümüş,",
    "verseNumber": 41,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  },
  {
    "id": 5800,
    "arabic": "أُو۟لَٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ",
    "turkish": "İşte onlardır kâfirler, haktan sapanlar.",
    "verseNumber": 42,
    "surahNumber": 80,
    "surahName": "سُورَةُ عَبَسَ",
    "englishName": "Abasa"
  }
]