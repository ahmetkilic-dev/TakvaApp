[
  {
    "id": 5932,
    "arabic": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلسَّمَآءِ وَٱلطَّارِقِ",
    "turkish": "Andolsun o göğe ve Târık'a,",
    "verseNumber": 1,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5933,
    "arabic": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ",
    "turkish": "Târık nedir, bildin mi?",
    "verseNumber": 2,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5934,
    "arabic": "ٱلنَّجْمُ ٱلثَّاقِبُ",
    "turkish": "O, karanlığı delen yıldızdır.",
    "verseNumber": 3,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5935,
    "arabic": "إِن كُلُّ نَفْسٍۢ لَّمَّا عَلَيْهَا حَافِظٌۭ",
    "turkish": "Hiçbir nefis yoktur ki başında bir denetleyici bulunmasın.",
    "verseNumber": 4,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5936,
    "arabic": "فَلْيَنظُرِ ٱلْإِنسَٰنُ مِمَّ خُلِقَ",
    "turkish": "Onun için insan neden yaratıldığına bir baksın.",
    "verseNumber": 5,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5937,
    "arabic": "خُلِقَ مِن مَّآءٍۢ دَافِقٍۢ",
    "turkish": "Atılan bir sudan yaratıldı.",
    "verseNumber": 6,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5938,
    "arabic": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ",
    "turkish": "O su, erkeğin sulbü ile kadının göğüs kemikleri arasından çıkar.",
    "verseNumber": 7,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5939,
    "arabic": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌۭ",
    "turkish": "Elbette Allah'ın onu döndürmeye gücü yeter.",
    "verseNumber": 8,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5940,
    "arabic": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ",
    "turkish": "O gün bütün sırlar yoklanıp, meydana çıkarılır.",
    "verseNumber": 9,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5941,
    "arabic": "فَمَا لَهُۥ مِن قُوَّةٍۢ وَلَا نَاصِرٍۢ",
    "turkish": "İnsanın o gün ne bir gücü vardır, ne de bir yardımcısı.",
    "verseNumber": 10,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5942,
    "arabic": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ",
    "turkish": "Andolsun o dönüşlü göğe,",
    "verseNumber": 11,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5943,
    "arabic": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ",
    "turkish": "O yarılıp çatlayan yere,",
    "verseNumber": 12,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5944,
    "arabic": "إِنَّهُۥ لَقَوْلٌۭ فَصْلٌۭ",
    "turkish": "Kuşkusuz Kur'ân, ayırıcı bir sözdür.",
    "verseNumber": 13,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5945,
    "arabic": "وَمَا هُوَ بِٱلْهَزْلِ",
    "turkish": "O asla bir şaka değildir.",
    "verseNumber": 14,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5946,
    "arabic": "إِنَّهُمْ يَكِيدُونَ كَيْدًۭا",
    "turkish": "Haberin olsun ki, kâfirler hep hile kuruyorlar.",
    "verseNumber": 15,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5947,
    "arabic": "وَأَكِيدُ كَيْدًۭا",
    "turkish": "Ben de hilelerine karşılık veririm.",
    "verseNumber": 16,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5948,
    "arabic": "فَمَهِّلِ ٱلْكَٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا",
    "turkish": "Onun için sen kâfirlere mühlet ver, onlara az bir zaman tanı.",
    "verseNumber": 17,
    "surahNumber": 86,
    "surahName": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq"
  },
  {
    "id": 5949,
    "arabic": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
    "turkish": "Rabbinin yüce adını tesbih et.",
