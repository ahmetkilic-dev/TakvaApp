    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5950,
    "arabic": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
    "turkish": "Yaratıp düzene koyan O'dur.",
    "verseNumber": 2,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5951,
    "arabic": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
    "turkish": "Takdir edip hidayeti gösteren O'dur.",
    "verseNumber": 3,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5952,
    "arabic": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
    "turkish": "Otlağı çıkaran,",
    "verseNumber": 4,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5953,
    "arabic": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
    "turkish": "Sonra da onu karamsı bir sel köpüğü haline getiren O'dur.",
    "verseNumber": 5,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5954,
    "arabic": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
    "turkish": "Bundan böyle sana Kur'ân'ı okutacağız da unutmayacaksın.",
    "verseNumber": 6,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5955,
    "arabic": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
    "turkish": "Yalnız Allah'ın dilediği başkadır. Çünkü o açığı da bilir, gizliyi de.",
    "verseNumber": 7,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5956,
    "arabic": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
    "turkish": "Seni en kolay yola muvaffak kılacağız.",
    "verseNumber": 8,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5957,
    "arabic": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
    "turkish": "Onun için öğüt ver, eğer öğüt fayda verirse.",
    "verseNumber": 9,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5958,
    "arabic": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
    "turkish": "Saygısı olan öğüt alacaktır.",
    "verseNumber": 10,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5959,
    "arabic": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
    "turkish": "Pek bedbaht olan da ondan kaçınacaktır.",
    "verseNumber": 11,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5960,
    "arabic": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
    "turkish": "O ki, en büyük ateşe girecektir.",
    "verseNumber": 12,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5961,
    "arabic": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
    "turkish": "Sonra ne ölecek onda, ne de hayat bulacaktır.",
    "verseNumber": 13,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
    "id": 5962,
    "arabic": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
    "turkish": "Doğrusu felah buldu (günahtan) temizlenen.  ",
    "verseNumber": 14,
    "surahNumber": 87,
    "surahName": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa"
  },
  {
